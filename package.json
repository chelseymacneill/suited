    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.1.0",
    "errorhandler": "^1.5.1",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.16.2",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
